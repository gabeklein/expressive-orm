{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/src/index.ts"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "globals": {
    "ts-jest": {
      "tsconfig": {
        "noUnusedLocals": false,
        "noUnusedParameters": false
      }
    }
  },
  "snapshotSerializers": [
    "./jest.config.js"
  ]
}